mixin contactTabInfo(state, rep, name, phone, phoneNoIcon)
  - var state = state || 'Washington'
  - var rep = rep || 'FCI'
  - var name = name || 'Rob Trickett'
  - var phone = phone || '425.483.1297'
  - var phoneNoIcon = phoneNoIcon || false
  .contact-info-item
    h2.contact-info-state=state
    .contact-wrap-info
      .contact-info-name=rep
      .contact-info-name=name
      a(href="tel:"+ phone).contact-info-phone=phone
      if(phoneNoIcon)
        .contact-info-phone.contact-info-phone--no-icon=phoneNoIcon