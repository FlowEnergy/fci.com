extends ../layout/layout
block vars
  - var title='DeltaPValve | Flow Control Industries | Quote Requests | General Inquiries'

block main-content
  +headerNav()
  .tab-scroll-wrap.margin-0
    #scroll-block.tab-scroll.tab-scroll--green
      .container.text-center
        ul.tab-scroll-list
          li: a.scroll-link-nav(href="#scroll-block-contact-1") contact
          li: a.scroll-link-nav(href="#scroll-block-contact-2") send message
          li: a.scroll-link-nav(href="#scroll-block-contact-3") representatives
  .wrap-contact-header
    .container
      .row
        .col-md-4.col-sm-6
          h3#scroll-block-contact-1.contact-title.animate-40 Call us
          .row
            .col-sm-6
              .contact-sub-title.animated-down-0-7s P:
              .contact-text.animated-down-0-9s 425.483.1297
            .col-sm-6
              .contact-sub-title.animated-down-0-7s F:
              .contact-text.animated-down-0-9s 425.486.5672
            .col-sm-6
              //- .contact-sub-title.animate-40 Toll-free:
              //- .contact-text.animated-down-0-9s 866.454.1288
        .col-md-3.col-sm-6.col-md-offset-1
          h3.contact-title.animate-40 Email us
          .contact-sub-title.animated-down-0-9s General Inquiries:
          a(href="mailto:sales@flowcontrol.com").contact-text.animated-down-0-9s sales@flowcontrol.com
          .contact-sub-title Service Issues:
          a(href="mailto:fciservice@flowcontrol.com").contact-text.animated-down-0-9s fciservice@flowcontrol.com
        .col-md-3.col-sm-6.col-md-offset-1
          h3.contact-title.animate-40 Visit us
          .contact-sub-title.animated-down-0-9s Address:
          a(href="#").contact-text.animated-down-0-7s 18715 141st Ave NE, Woodinville, WA 98072



  .container.text-center
    h1#scroll-block-contact-2.custom-title.animate-40 General Contact

    form(id='contactForm')
      .row
        .col-md-12
          .form-group.animated-down-0-9s
            input.form-control(type="text" placeholder="Name" id="contactName" required)
        .col-md-6
          .form-group.animated-down-0-9s
            input.form-control(type="text" placeholder="Email" id="contactEmail" required)
        .col-md-6
          .form-group.animated-down-0-9s
            input.form-control(type="text" placeholder="Phone" id="contactPhone" required)
      .row.row-textarea
        .col-md-12
          .form-group.animated-down-0-9s
            textarea.form-control.textarea-contact(placeholder="Message" id="contactMessage" required)
      p.content-text.animated-down-0-9s For quote requests please include project details.
      br.hidden-xs.hidden-sm

      .row
        .col-xs-12
          .green-label.animated-down-0-9s Quote Requests
      .row
        .col-md-4.col-sm-6
          .form-group.animated-down-0-9s
            input.form-control(type="text" placeholder="Project Name" id="projectName")
        .col-md-4.col-sm-6
          .form-group.animated-down-0-9s
            input.form-control(type="text" placeholder="Project Location" id="projectLocation")
        .col-md-4.col-sm-12.animated-down-0-9s
          .form-group
            input.form-control(type="text" placeholder="Design Engineer" id="designEngineer")
      .row
        .col-md-6
          .form-group.animated-down-0-9s
            input.form-control(type="text" placeholder="Equipment Schedule & Specification" id="projectEquipment")
        .col-md-6
          .form-group.animated-down-0-9s
            input.form-control(type="text" placeholder="New or Retrofit Construction" id="projectType")
      .row
        .col-md-12.text-right
            button.animated-down-0-9s.btn.btn-main.btn-contact-send-message.animate-40(type='button' id='contactButton') send message

    hr
