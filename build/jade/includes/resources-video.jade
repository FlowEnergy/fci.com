.row
  .col-md-10.col-md-offset-1.text-left
    ul.nav.nav-tabs.nav-resources(role='tablist')
      li.active(role='presentation')
        a(href='#video', aria-controls='video', role='tab', data-toggle='tab') videos
      li(role='presentation')
        a(href='#webinars', aria-controls='webinars', role='tab', data-toggle='tab') webinars
  .col-md-1
.tab-content
  #video.tab-pane.active(role='tabpanel')
    .row
      .col-sm-1
        a.left.carousel-control.resources-control(href='#resources-video-carousel', role='button', data-slide='prev')
          img.svg(src="../img/resources/resources-arrow.svg" alt="")
      .col-md-10
        #resources-video-carousel.resources-video-carousel.carousel.slide(data-ride='carousel')
          .carousel-inner(role='listbox')
            .item.active
              .row
                .col-md-6
                  +resourcesVideoItem('https://www.youtube.com/embed/i1gU-yX-J7g?origin=http://flowcontrol.com&rel=0', 'Changing Control Characteristics: Stem Replacement')
                .col-md-6
                  +resourcesVideoItem('https://www.youtube.com/embed/XS_rMu5JiYU?origin=http://flowcontrol.com&rel=0', 'Changing Control Characteristics: Cv Disk Replacement')
            .item
              .row
                .col-md-6
                  +resourcesVideoItem('https://www.youtube.com/embed/M20zj4DbuII?origin=http://flowcontrol.com&rel=0', 'DeltaPValve Commissioning Checklist')
                .col-md-6
                  +resourcesVideoItem('https://www.youtube.com/embed/BS4-6z5Ndqs?origin=http://flowcontrol.com&rel=0', 'Verifying Pressure Independence')
            .item
              .row
                .col-md-6
                  +resourcesVideoItem('https://www.youtube.com/embed/_Pe4gWDfOss?origin=http://flowcontrol.com&rel=0', 'Shutoff Confirmation')
                .col-md-6
                  +resourcesVideoItem('https://www.youtube.com/embed/pM5Op1hrEz4?origin=http://flowcontrol.com&rel=0', 'Test Stand Demonstration')
            .item
              .row
                .col-md-6
                  +resourcesVideoItem('https://www.youtube.com/embed/DP3HRi6CyCI?origin=http://flowcontrol.com&rel=0', 'DeltaPValve Pressure Regulator')
                .col-md-6
                  +resourcesVideoItem('https://www.youtube.com/embed/nHSPv1HMbtA?origin=http://flowcontrol.com&rel=0', 'DeltaPValve Control Surface')
      .col-md-1
        a.right.carousel-control.resources-control(href='#resources-video-carousel', role='button', data-slide='next')
          img.svg(src="../img/resources/resources-arrow.svg" alt="")
  #webinars.tab-pane(role='tabpanel')
    .row
      .col-sm-1
        //a.left.carousel-control.resources-control(href='#resources-webinars-carousel', role='button', data-slide='prev')
          img.svg(src="../img/resources/resources-arrow.svg" alt="")
      .col-sm-10
        #resources-webinars-carousel.resources-video-carousel.carousel.slide(data-ride='carousel')
          .carousel-inner(role='listbox')
            .item.active
              .row
                .col-sm-6
                  +resourcesVideoItem('https://www.youtube.com/embed/QEKtxloasdk?origin=http://flowcontrol.com&rel=0', 'Precision Control')
